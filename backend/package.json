        "cloudinary": "^1.41.3",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-validator": "^7.0.1",
        "firebase-admin": "^13.5.0",
        "google-auth-library": "^10.4.1",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
